<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <activeProfiles>
    <activeProfile>REPLACEWITHPROFILENAME</activeProfile>
  </activeProfiles>

   <profiles>
		<profile>
			<id>REPLACEWITHPROFILENAME</id>
			<repositories>
				<repository>
					<id>central</id>
					<url>https://repo1.maven.org/maven2</url>
					<releases><enabled>true</enabled></releases>
					<snapshots><enabled>true</enabled></snapshots>
			</repository>
				<repository>
					<id>ArtifactRepository</id>
					<name>ATI Automation Core</name>
					<url>ArtifactRepositoryURL</url>
				</repository>
				<repository>
					<id>REPLACEWITHPROJECTNAME-PAGES</id>
					<name>REPLACEWITHPROJECTNAME</name>
					<url>ProjectArtifactRepositoryURL</url>
				</repository>
				<!-- Add any other needed artifact repos for Pages-->
			</repositories>
		</profile>
	</profiles>
  <servers>
	<!-- Add any other needed credentials for artifact repos for Pages-->
	<server>
      <id>ArtifactRepository</id>
	  <username>REPLACEWITHUSERNAME</username>
      <password>REPLACEWITHAPPROPRIATEPASSWORD</password>
    </server>
	<server>
      <id>REPLACEWITHPROJECTNAME-PAGES</id>
	  <username>REPLACEWITHUSERNAME</username>
      <password>REPLACEWITHAPPROPRIATEPASSWORD</password>
    </server>
  </servers>
</settings>